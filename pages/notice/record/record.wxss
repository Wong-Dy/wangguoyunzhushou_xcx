
page {
  background-color: #f8f8f8;
  height: 100%;
}

.scroll-view {
  display: flex;
  flex-direction: column;
  width: 100%;
  margin: 10px 0;
}

.person-line {
  width: 100%;
  height: 1px;
  background-color: #f4f4f4;
}

.person-line-radio {
  width: 100%;
  height: 15rpx;
  background-color: #f4f4f4;
}

.list-item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background-color: white;
  padding: 10px 15px;
}

.list-view-left {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}

.list-view-right {
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: right;
}

.list-text-first {
  font-size: 14px;
}

.list-text-last {
  font-size: 12px;
}

.list-text-first1 {
  font-size: 14px;
  color: gray;
}

.list-text-last1 {
  font-size: 16px;
}

.not-data-tip {
  width: 100%;
  z-index: -100;
  margin-top: 45%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.not-data-tip text {
  font-size: 14px;
  margin-top: 10px;
  color: dimgray;
}
